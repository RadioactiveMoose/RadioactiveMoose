turbulenceDamping
{
    type            compressible::VoFTurbulenceDamping;

    delta           1e-4;
}


VoFBoiling
{
    type    compressible::VoFBoiling;
    
    model   Lee;
    
    LeeCoeffs
    {
        // liquid should NOT be here 
        liquid  water;  // This should be at VoFBoiling level, NOT inside LeeCoeffs
    	Tsat	373.15;
        tInf    1;
        Cv      0.5;
        Cc      0.8;
    }
}

